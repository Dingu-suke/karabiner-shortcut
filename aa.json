{
  "title": "コントロールと矢印でタブ移動",
  "rules": [
      {
          "description": "左矢印",
          "manipulators": [
              {
                  "type": "basic",
                  "from": {
                      "key_code": "left_arrow",
                      "modifiers": {
                          "mandatory": ["left_control"],
                          "optional": ["any"]
                      }
                  },
                  "to": [
                      {
                        "key_code": "close_bracket",
                        "modifiers": ["left_command", "right_shift"]
                      }
                  ]
              }
          ]
      }
  ]
}